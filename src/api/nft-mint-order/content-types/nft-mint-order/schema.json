{
  "kind": "collectionType",
  "collectionName": "nft_mint_orders",
  "info": {
    "singularName": "nft-mint-order",
    "pluralName": "nft-mint-orders",
    "displayName": "NFT Mint Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "sendAddress": {
      "type": "string"
    },
    "blockchain": {
      "type": "enumeration",
      "enum": [
        "ETH",
        "BSC",
        "MATIC",
        "CELO",
        "ONE"
      ]
    },
    "tikTokVideoId": {
      "type": "string",
      "unique": true
    },
    "extraMessage": {
      "type": "string"
    },
    "royalties": {
      "type": "json",
      "sampleData": {
        "isSplitRoyaltyRate": false,
        "singleAddress": "0x0000",
        "royaltyRate": 0.1,
        "splitAddress  -> * isSplitRoyaltyRate === true": [
          {
            "address": "0x0000",
            "splitRoyaltyRate": 0.1
          }
        ]
      }
    },
    "transactionId": {
      "type": "string",
      "unique": true
    },
    "signatureId": {
      "type": "string",
      "unique": true
    },
    "contractAddress": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
